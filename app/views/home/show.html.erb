<section id="main" class="container 75%">
	<header>
		<h2>Contact Us</h2>
		<p>Ask us or tell us what you think about our operation.</p>
	</header>
	<div class="box">
		<form action="http://vestmunity.us12.list-manage.com/subscribe/post?u=3d279e1c62ab7996c31b033c1&amp;id=d3e1f940af" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="validate" target="_blank" novalidate>
			<div class="row uniform 50%">
				<div class="6u 12u(mobilep)">
					<input type="text" name="NAME" class="" id="mce-NAME" value="" placeholder="Name" />
				</div>
				<div class="6u 12u(mobilep)">
					<input type="email" name="EMAIL" class="required email" id="mce-EMAIL" value="" placeholder="Email" />
				</div>
			</div>
			<div class="row uniform 50%">
				<div class="6u 12u(mobilep)">
					<input type="text" name="PHONE" class="" id="mce-PHONE" value="" placeholder="Phone Number" />
				</div>
				<div class="6u 12u(mobilep)">
					<input type="text" name="subject" id="subject" value="" placeholder="Subject" />
				</div>
			</div>
			<div class="row uniform 50%">
				<div class="12u">
					<textarea name="message" name="MMERGE4" id="mce-MMERGE4" placeholder="Enter your message" rows="6"></textarea>
				</div>
			</div>
			<div id="mce-responses" class="clear">
				<div class="response" id="mce-error-response" style="display:none"></div>
				<div class="response" id="mce-success-response" style="display:none"></div>
			</div>    <!-- real people should not fill this in and expect good things - do not remove this or risk form bot signups-->
			<div class="row uniform">
				<div class="12u">
					<ul class="actions align-center">
						<li><input type="submit" value="Send Message" name="subscribe" id="mc-embedded-subscribe" class="button" /></li>
					</ul>
				</div>
			</div>
		</form>
	</div>
</section>

