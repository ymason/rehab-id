<header id="header">
	<h1><%= link_to 'RehabID', root_path%> - Automatically identify your next rehab project.</h1>
	<% if user_signed_in? %>
		<nav id="nav">
			<ul>
				<li><%= link_to 'Dashboard', dashboard_path%></li>
				<li><%= link_to 'Home', root_path%></li>
				<li><%= link_to 'Contact', contact_path%></li>
				<li><%= link_to 'Logout', destroy_user_session_path(:authenticity_token => form_authenticity_token()), method: 'delete' %></li>
			</ul>
		</nav>
	<% else %>
		<nav id="nav">
			<ul>
				<li><%= link_to 'Home', root_path%></li>
				<li><%= link_to 'Contact', contact_path%></li>
				<li><%= link_to 'Sign Up', new_user_registration_path%></li>
				<li><%= link_to 'Login', new_user_session_path%></li>
			</ul>
		</nav>
	<% end %>

</header>